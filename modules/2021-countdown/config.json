{
  "filename": "config.json",
  "content": [
    {
      "field_name": "channel_id",
      "default": "",
      "type": "channelID",
      "description": "Channel-ID of the countdown channel"
    },
    {
      "field_name": "format",
      "default": "%d%:%h%:%m%:%s% left",
      "type": "string",
      "description": "Content of countdown channel",
      "params": [
        {
          "name": "%d%",
          "description": "Days left"
        },
        {
          "name": "%h%",
          "description": "Hours left"
        },
        {
          "name": "%m%",
          "description": "Minutes left"
        },
        {
          "name": "%s%",
          "description": "Seconds left"
        }
      ]
    },
    {
      "field_name": "update_interval",
      "default": 30,
      "type": "integer",
      "description": "Set the interval of changing the channel name in seconds. Can not be under 30."
    },
    {
      "field_name": "2021_text",
      "default": "Happy 2021 \uD83C\uDF89",
      "type": "string",
      "description": "Content of channel when new year starts!"
    }
  ]
}